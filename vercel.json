
{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-php@0.7.4"
        }
    ],
    "routes": [
        {
            "src": "/storage/(.*)",
            "dest": "public/storage/$1"
        },
        {
            "src": "/(css|js|img)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/build/(.*)",
            "dest": "public/build/$1"
        },
        {
            "src": "/(.*\\.(ico|png|jpg|jpeg|gif|svg|webp|css|js|json|txt|xml|woff|woff2|ttf|eot))",
            "dest": "public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.php"
        }
    ]
}
